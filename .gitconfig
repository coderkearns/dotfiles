[credential]
	helper = store
[user]
	email = coder.kearns@gmail.com
	name = coderkearns
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
	default = current
[pull]
	rebase = false
[core]
	editor = nano
[alias]
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"
    undo = "reset --soft HARD~1"
    ouch = "reset --hard HARD~1"
    typo = "commit --amend"
    unstage = "restore --staged"
