[credential]
	helper = store
[user]
	email = coder.kearns@gmail.com
	name = coderkearns
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
	default = current
[pull]
	rebase = false
[core]
	editor = nano
[alias]
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"
    undo = "reset --soft HEAD~1"
    ouch = "reset --hard HEAD~1"
    typo = "commit --amend"
    unstage = "restore --staged"
    branches = "branch -l"
    conflicts = "diff --name-only --diff-filter=U"
    prune="remote prune origin"
